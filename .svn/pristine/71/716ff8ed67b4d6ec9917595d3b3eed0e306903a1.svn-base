<html lang="en">

<head>
   <meta charset="UTF-8">
   <base href="../">
   <meta http-equiv="content-type" content="text/html;charset=utf-8;" />
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel="stylesheet" href="styles/plugins/elementui.css">
   <link rel="stylesheet" href="components/css-loader.css">
   <link rel="stylesheet" href="styles/plugins/font-awesome.css">
   <link rel="stylesheet" href="styles/style.css">

   <title>组件开发环境</title>
   <style>
   </style>
</head>

<body>
   <div id="app">
      <el-row>
         <el-col :span="16">
            <el-card class="box-card">
               <div slot="header" class="clearfix title">
                  <span>
                     <b>树显示</b>：组件测试条件（包括props、内部条件等）</span>
               </div>
               <div>
                  <table border="1" style="width: 100%;">
                     <tr>
                        <td>参数</td>
                        <td>方向</td>
                        <td>说明</td>
                        <td>类型</td>
                        <td>可选值</td>
                        <td>默认值</td>
                     </tr>
                     <tr>
                        <td>showcheckbox</td>
                        <td>输入</td>
                        <td>是否多选(不需要测试)</td>
                        <td>boolean</td>
                        <td>
                           <el-select v-model="showcheckbox" placeholder="请选择">
                              <el-option
                                v-for="item in [{label:'true',value:true},{label:'false',value:false}]"
                                :key="item.value"
                                :label="item.label"
                                :value="item.value">
                              </el-option>
                           </el-select>
                        </td>
                        <td>false</td>
                     </tr>
                     <!-- <tr>
                        <td>defaultkeys</td>
                        <td>输入</td>
                        <td>默认展开节点</td>
                        <td>array</td>
                        <td>
                           <input v-model="defaultkeys">
                        </td>
                        <td></td>
                     </tr> -->
                     <tr>
                        <td>fulltreeids</td>
                        <td>输入/输出</td>
                        <td>选中、半选中状态数据的</td>
                        <td>array</td>
                        <td>
                           <input v-model="fulltreeids">
                        </td>
                        <td></td>
                     </tr>
                     <tr>
                        <td>fulltreenode</td>
                        <td>输出</td>
                        <td>选中、半选中状态数据</td>
                        <td>array</td>
                        <td>
                           <input type="text" v-model="fulltreenode">
                        </td>
                        <td></td>
                     </tr>
                     <tr>
                        <td>treeids</td>
                        <td>输入/输出</td>
                        <td>选中叶子节点数据的Id</td>
                        <td>array</td>
                        <td>
                           <input v-model="treeids">
                        </td>
                        <td></td>
                     </tr>
                     <tr>
                        <td>treenode</td>
                        <td>输出</td>
                        <td>选中叶子节点数据</td>
                        <td>array</td>
                        <td>
                           <input v-model="treenode">
                        </td>
                        <td></td>
                     </tr>
                  </table>
               </div>
            </el-card>
            <el-card class="box-card" style="margin-top: 10px;">
               <slot>
                  <div slot="header" class="clearfix">
                     <span>组件显示</span>
                  </div>
               </slot>
               <div>
                  <el-row>
                     <el-col :span="12">
                        <vx-basic-tree :showcheckbox="showcheckbox" :defaultprops="defaultprops" :defaultkeys="defaultkeys" :options="options" :fulltreeids.sync="fulltreeids"
                           :treeids.sync="treeids" :treenode.sync="treenode" :fulltreenode.sync="fulltreenode" :currentnode.sync="currentnode"></vx-basic-tree>
                     </el-col>
                     <!-- <el-col :span="12">
                        <vx-basic-tree :showcheckbox="false" :root="root" :data="data" :fulltreeids.aync="fulltreeids" :treeids.aync="treeids" :treenode.aync="treenode"
                           :fulltreenode.aync="fulltreenode"></vx-basic-tree>
                     </el-col> -->
                  </el-row>
               </div>
            </el-card>
            <el-card class=" box-card" style="margin-top: 10px;">
               <div slot="header" class="clearfix title">
                  <span>组件数据变量（包括输出结果、内部数据等）</span>
               </div>
               <div>
                  <!-- 输出的结果与内部数据 -->
                  <div>选中、半选中状态数据： {{fulltreenode}}</div>
                  <div>选中、半选中状态数据的Id： {{fulltreeids}}</div>
                  <div>选中叶子节点数据： {{fulltreenode}}</div>
                  <div>选中叶子节点数据的Id： {{treeids}}</div>
                  <div>当前点击节点： {{currentnode}}</div>
               </div>
            </el-card>
         </el-col>
         <el-col :span="8">
            <el-card class="box-card" style="margin-left: 10px;">
               <div slot="header" class="clearfix">
                  <span>自动化测试脚本</span>
                  <el-button style="float: right; padding: 3px 0" type="text">开始测试</el-button>
               </div>
               <textarea style="border-radius: 5px; width: 100%; height: 80%;"></textarea>
            </el-card>
         </el-col>
      </el-row>
   </div>
</body>

<script type="text/javascript" src="scripts/plugins/echarts.js"></script>
<!-- 先引入 Vue -->
<script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
<script type="text/javascript" src="scripts/plugins/vuex.js"></script>
<!-- 引入组件库 -->
<script type="text/javascript" src="scripts/plugins/elementui.min.js"></script>

<!-- 常用控件库 -->
<script type="text/javascript" src="scripts/plugins/jquery-2.0.3.min.js"></script>
<script type="text/javascript" src="scripts/plugins/moment/moment.min.js"></script>
<script type="text/javascript" src="scripts/plugins/moment/moment.zh-cn.js"></script>

<!-- 自定义JS库 -->
<script type="text/javascript" src="scripts/common/jquery.ajax.js"></script>
<script type="text/javascript" src="scripts/common/components-init.js"></script>
<script>
   vueOptions = {
      data() {
         return {
            Search: "测试条件",
            PageTitle: "组件开发环境",
            fulltreeids: [],/// 包括路径的选中菜单数据Id []
            defaultkeys: [], /// 默认展开节点的 key
            treeids: [1], /// 选中的菜单数据Id []
            showcheckbox: true, /// 是否多选 true/false
            treenode: [], ///选中的菜单数据 []
            currentnode: {}, /// 当前节点
            fulltreenode: [], ///包括路径的选中菜单数据 []
            options: [
               {
                  Id: 1,
                  label: "一级 1",
                  children: [
                     {
                        Id: 4,
                        label: "二级 1-1",
                        children: [
                           {
                              Id: 9,
                              label: "三级 1-1-1"
                           },
                           {
                              Id: 10,
                              label: "三级 1-1-2"
                           }
                        ]
                     }
                  ]
               },
               {
                  Id: 2,
                  label: "一级 2",
                  children: [
                     {
                        Id: 5,
                        label: "二级 2-1"
                     },
                     {
                        Id: 6,
                        label: "二级 2-2"
                     }
                  ]
               },
               {
                  Id: 3,
                  label: "一级 3",
                  children: [
                     {
                        Id: 7,
                        label: "二级 3-1"
                     },
                     {
                        Id: 8,
                        label: "二级 3-2"
                     }
                  ]
               }
            ],
            defaultprops: {
               children: 'children', ///指定子树为节点对象的某个属性值
               label: 'label' ///指定节点标签为节点对象的某个属性值
            }
         }
      },
      created() {

      },
      watch: {
         fulltreeids: {
            handler: function (val, oldVal) {
               console.log("fulltreeids")
               console.log(this.fulltreeids)
            },
            deep: true
         },
         fulltreenode: {
            handler: function (val, oldVal) {
               console.log("fulltreenode")
               console.log(this.fulltreenode)
            },
            deep: true
         },
         treeids: {
            handler: function (val, oldVal) {
               console.log("treeids")
               console.log(this.treeids)
            },
            deep: true
         },
         treenode: {
            handler: function (val, oldVal) {
               console.log("treenode")
               console.log(this.treenode)
            },
            deep: true
         },
         currentnode: {
            handler: function (val, oldVal) {
               console.log("currentnode")
               console.log(this.currentnode)
            },
            deep: true
         }
      },

      methods: {
      }
   }
</script>
<style>
   table {
      /* width: 100%; */
      border-collapse: separate;
      border-spacing: 2;
   }
</style>

</html>
